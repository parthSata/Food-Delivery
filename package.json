    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/validator": "^13.11.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
